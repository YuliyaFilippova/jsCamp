{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-classes-per-file": [
            "error",
            10
        ], 
        "no-underscore-dangle" : [
            "error", 
            {
                "allowAfterThis": true
            }
        ] 

    }
}
